<Project>

	<PropertyGroup Label="Basic Settings">
		<VersionPrefix>0.1.0-preview1</VersionPrefix>
		<TreatWarningsAsErrors>false</TreatWarningsAsErrors>
		<SuppressNETCoreSdkPreviewMessage>true</SuppressNETCoreSdkPreviewMessage>
		<NoWarn>$(NoWarn);CS1591;NU1605;</NoWarn>
	</PropertyGroup>

	<PropertyGroup>
		<LangVersion>preview</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<PropertyGroup Label="Ownership Settings">
		<Company>King David Consulting LLC</Company>
		<Authors>kdcllc</Authors>
		<Copyright>Copyright Â© King David Consulting LLC</Copyright>
	</PropertyGroup>

	<PropertyGroup Label="Nuget Package Settings">
		<Description>The DotNetCore Implementation of Walmart Seller Apis.</Description>
		<PackageProjectUrl>https://github.com/kdcllc/Bet.Extensions.Walmart</PackageProjectUrl>
		<RepositoryUrl>https://github.com/kdcllc/Bet.Extensions.Walmart.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageLicense>https://github.com/kdcllc/Bet.Extensions.Walmart/master/LICENSE</PackageLicense>
		<PackageIcon>icon.png</PackageIcon>
		<PackageTags>dotnetcore, aspnetcore, dotnet, walmart seller, walmart, e-commerce</PackageTags>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
	</PropertyGroup>

	<PropertyGroup Label="SourceLink Settings">
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
		<DebugType>Embedded</DebugType>
		<EmbedAllSources>True</EmbedAllSources>
	</PropertyGroup>

</Project>
